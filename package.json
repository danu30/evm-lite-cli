{
  "name": "evm-lite-cli",
  "version": "2.0.0",
  "main": "dist/index.js",
  "typings": "dist/typings/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p . -c ./tslint.json",
    "prepublish": "yarn link && yarn build",
    "clean": "rm -rf node_modules dist package-lock.json bin",
    "build": "yarn lint && tsc -p ./tsconfig.json",
    "build:watch": "npm run build --watch",
    "test": "jest",
    "dev": "yarn install --registry http://192.168.1.4:4873",
    "publish:dev": "yarn publish --registry http://192.168.1.4:4873",
    "add:dev": "yarn add --registry http://192.168.1.4:4873",
    "i": "ts-node src/evmlc i",
    "e": "ts-node src/evmlc",
    "linkalldev": "yarn link evm-lite-core evm-lite-consensus evm-lite-keystore evm-lite-datadir evm-lite-client evm-lite-utils",
    "pkg": "yarn run build && rm -rf bin && pkg . --out-path ./bin"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.0",
    "mkdirp": "^0.5.1",
    "npmlog": "^4.1.2",
    "vorpal": "^1.12.0",
    "evm-lite-client": "^1.3.2",
    "evm-lite-consensus": "^1.3.2",
    "evm-lite-core": "^1.3.2",
    "evm-lite-datadir": "^1.3.3",
    "evm-lite-keystore": "^1.3.2",
    "evm-lite-utils": "^1.3.3"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.18",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.7.5",
    "@types/npmlog": "^4.1.2",
    "edit-json-file": "^1.3.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "bin": {
    "evmlc": "./dist/evmlc.js"
  },
  "pkg": {
    "assets": [
      "./node_modules/figlet/fonts/Standard.flf"
    ],
    "out-path": "./bin"
  }
}
