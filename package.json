{
	"name": "evm-lite-cli",
	"version": "1.0.4",
	"main": "dist/index.js",
	"typings": "dist/typings/index.d.ts",
	"license": "MIT",
	"scripts": {
		"lint": "tslint -p . -c ./tslint.json",
		"prepublish": "yarn link && yarn build",
		"clean": "rm -rf node_modules && rm -rf dist/ && rm -rf package-lock.json",
		"build": "tsc -p tsconfig.json",
		"build:watch": "npm run build --watch",
		"test": "jest",
		"dev": "yarn install --registry http://192.168.1.4:4873",
		"publish:dev": "yarn publish --registry http://192.168.1.4:4873",
		"add:dev": "yarn add --registry http://192.168.1.4:4873",
		"i": "ts-node src/cli i --debug",
		"e": "ts-node src/cli",
		"linkalldev": "yarn link evm-lite-core evm-lite-consensus evm-lite-keystore evm-lite-datadir evm-lite-client evm-lite-utils",
		"addall": "yarn add evm-lite-core evm-lite-consensus evm-lite-keystore evm-lite-datadir evm-lite-client evm-lite-utils"
	},
	"dependencies": {
		"ascii-table": "^0.0.9",
		"chalk": "^2.4.2",
		"dotenv": "^8.0.0",
		"evm-lite-client": "^1.3.0",
		"evm-lite-consensus": "^1.3.0",
		"evm-lite-core": "^1.3.0",
		"evm-lite-datadir": "^1.3.0",
		"evm-lite-keystore": "^1.3.0",
		"evm-lite-utils": "^1.3.0",
		"figlet": "^1.2.3",
		"inquirer": "^6.4.1",
		"mkdirp": "^0.5.1",
		"vorpal": "^1.12.0"
	},
	"devDependencies": {
		"@types/figlet": "^1.2.0",
		"@types/inquirer": "^6.0.3",
		"@types/jest": "^24.0.15",
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.0.10",
		"edit-json-file": "^1.2.1",
		"jest": "^24.8.0",
		"ts-jest": "^24.0.2",
		"ts-node": "^8.3.0",
		"tslint": "^5.18.0",
		"tslint-config-airbnb": "^5.11.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.5.2"
	},
	"bin": {
		"evmlc": "./dist/cli.js"
	}
}
